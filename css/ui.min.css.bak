.health-bar{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;width:auto;height:auto;padding:.5rem;background:#fff;position:relative}.health-bar span{position:absolute}.health-bar .bar{background:#CC5544;width:100%;height:10px;position:relative;transition:width .5s linear}.health-bar .hit{background:rgba(255,255,255,0.6);position:absolute;top:0;right:0;bottom:0;width:0px;transition:width .5s linear}.mana-bar{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;width:auto;height:auto;padding:.5rem;background:#fff;position:relative}.mana-bar span{position:absolute}.mana-bar .bar{background:#2196f3;width:100%;height:10px;position:relative;transition:width .5s linear}.mana-bar .hit{background:rgba(255,255,255,0.6);position:absolute;top:0;right:0;bottom:0;width:0px;transition:width .5s linear}body{background-color:#EEEDF2;color:#000811 !important}#loginBtn{background-color:#666D7F;border-radius:50px;margin:2rem auto 0 auto;color:#EEEDF2;display:block;height:50px;font-size:1.5rem}#status,#salvarSingle{display:none}#return{display:none}#return .card-content>a{position:absolute;right:-20px;top:50%;bottom:auto;transform:translateY(-50%);background-color:#fff !important;color:#222}#editBox{position:absolute;top:0;left:0;margin:0;background-color:#ffffff}#editBox .remove_field{background-color:#ffffff;right:24px;bottom:-26px;z-index:99;box-shadow:none}#userInfo{position:relative;min-height:100px;background-color:rgba(255,0,0,0.5);margin-bottom:50px}#userInfo .imgUser{position:relative;display:block;width:70px;height:70px;background-color:#eee;border-radius:100%;top:20px;overflow:hidden}#userInfo div:last-child h1{font-size:1.2rem;margin-bottom:0}.bar input{position:absolute;margin:0;height:auto;top:-20px;width:50%;border:transparent}.bar input:last-child{right:0}img{max-width:100%}
